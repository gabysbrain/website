	Issn = {1077-2626},
	Journal = {IEEE Transactions on Visualization and Computer Graphics},
	Month = jan,
	Number = {1},
	Pages = {791-800},
	Title = {Urban Pulse: Capturing the Rhythm of Cities},
	Volume = {23},
