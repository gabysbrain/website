	Journal = {IEEE Transactions on Visualization and Computer Graphics},
	Number = {12},
	Pages = {2226--2235},
	Rating = {0},
	Title = {SeiVis: An Interactive Visual Subsurface Modeling Application},
	Url = {http://ieeexplore.ieee.org/xpl/articleDetails.jsp?tp=&arnumber=6327227&contentType=Journals+%26+Magazines&matchBoolean%3Dtrue%26rowsPerPage%3D30%26searchField%3DSearch_All%26queryText%3D%28p_Title%3A%22SeiVis%3A+An+Interactive+Visual+Subsurface+Modeling+Application%22%29},
