	Journal = {IEEE Transactions on Visualization and Computer Graphics},
	Number = {12},
	Pages = {2014--2022},
	Rating = {0},
	Title = {Computing {M}orse-{S}male Complexes with Accurate Geometry},
	Url = {http://ieeexplore.ieee.org/xpl/articleDetails.jsp?tp=&arnumber=6327205&contentType=Journals+%26+Magazines&matchBoolean%3Dtrue%26rowsPerPage%3D30%26searchField%3DSearch_All%26queryText%3D%28p_Title%3A%22Computing+Morse-Smale+Complexes+with+Accurate+Geometry%22%29},
