	Issn = {1077-2626},
	Journal = {IEEE Transactions on Visualization and Computer Graphics},
	Month = oct,
	Number = {10},
	Pages = {1393--1406},
	Title = {Positional uncertainty of isocontours: Condition analysis and probabilistic measures},
