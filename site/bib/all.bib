	Doi = {10.1109/TVCG.2008.119},
	Journal = {IEEE Transactions on Visualization and Computer Graphics},
	Month = dec,
	Number = {6},
	Pages = {1428--1435},
	Pmid = {18988993},
	Rating = {4},
