@article{Ingalls:2008,
	Abstract = {Sensitivity analysis addresses the manner in which model behaviour depends on model parametrization. Global sensitivity analysis makes use of statistical tools to address system behaviour over a wide range of operating conditions, whereas local sensitivity analysis focuses attention on a specific set of nominal parameter values. This narrow focus allows a complete analytical treatment and straightforward interpretation in the local case. Sensitivity analysis is a valuable tool for model construction and interpretation, and can be applied in medicine and biotechnology to predict the effect of interventions.},
	Address = {Department of Applied Mathematics, University of Waterloo, Waterloo, Ontario, Canada N2L 3G1. bingalls@math.uwaterloo.ca},
	Annote = {Talks about local vs global sensitivity analysis in the context of systems biology.  The argue that local analysis is good for seeing the sensitivities of parameters around an optimum region while global analysis is good for finding general trends amongst the parameters and narrowing down the scope of large models.},
	Author = {Ingalls, Brian},
