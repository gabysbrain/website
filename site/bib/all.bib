	Month = feb,
	Number = {2},
	Pages = {291-305},
	Title = {TripAdvisor\textsuperscript{N-D}: A Tourism-Inspired High-Dimensional Space Exploration Framework with Overview and Detail},
	Volume = {19},
	Year = {2013},
	
	}

